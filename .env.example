# Ultimate Math Agent - Environment Variables
# Copy this file to .env and fill in your API keys

# OpenAI (GPT-5.2 Pro)
OPENAI_API_KEY=sk-your-openai-key
OPENAI_MODEL=gpt-5.2-pro

# xAI (Grok-4.2)
XAI_API_KEY=xai-your-key
XAI_MODEL=grok-4.2-heavy

# Google (Gemini 3)
GOOGLE_API_KEY=your-google-key
GEMINI_MODEL=gemini-3-pro

# Anthropic (Claude Opus 4.5)
ANTHROPIC_API_KEY=sk-ant-your-key
CLAUDE_MODEL=claude-opus-4.5

# DeepSeek (for math proof refinement)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_MODEL=deepseek-chat

# Harmonic AI / Aristotle (for Lean4 formal proofs)
HARMONIC_API_KEY=your_harmonic_api_key_here
ARISTOTLE_MODEL=aristotle-lean4
HARMONIC_API_BASE=https://api.harmonic.ai/v1h-v2

# Lean4 (Optional)
LEAN4_PATH=/usr/local/bin/lean
LEAN4_PROJECT_PATH=./lean_proofs

# Pipeline Configuration
MAX_ITERATIONS=5
CONFIDENCE_THRESHOLD=0.9
VERBOSE=true
